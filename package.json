{
    "name": "react-leaflet",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build --config vite.config.ts && vite build --config vite.preview.ts",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite build -c vite.preview.ts && vite preview"
    },
    "dependencies": {
        "leaflet": "^1.9.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-syntax-highlighter": "^15.5.0"
    },
    "devDependencies": {
        "@types/leaflet": "^1.9.8",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@types/react-syntax-highlighter": "^15.5.11",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.55.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vite-plugin-dts": "^3.7.2"
    },
    "main": "./dist/react-leaflet.umd.cjs",
    "module": "./dist/react-leaflet.js",
    "exports": {
        ".": {
            "import": "./dist/react-leaflet.js",
            "require": "./dist/react-leaflet.umd.cjs"
        }
    }
}
